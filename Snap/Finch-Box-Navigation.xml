ction getSensorData(callback) {&#xD;&#xD;&#xD;    var getValue = function(response) {&#xD;&#xD;&#xD;        sensorvalue = response.yAcc;&#xD;        callback(sensorvalue);&#xD;&#xD;&#xD;    };&#xD;&#xD;&#xD;&#xD;    console.log("sending message to chrome");&#xD;&#xD;&#xD;    chrome.runtime.sendMessage(finchAppID, {&#xD;&#xD;&#xD;        message: "POLL"&#xD;&#xD;&#xD;    }, getValue);&#xD;&#xD;&#xD;    console.log("sent message to chrome");&#xD;&#xD;&#xD;};&#xD;&#xD;&#xD;&#xD;&#xD;&#xD;function processValue(sensorvalue) {&#xD;&#xD;&#xD;    console.log("sensor value has been set: ", sensorvalue);&#xD;&#xD;&#xD;    myself.finchDummy4 = sensorvalue;&#xD;};&#xD;&#xD;&#xD;&#xD;&#xD;getSensorData(processValue);&#xD;&#xD;</l></block><list></list></block><block s="doWait"><l>0.01</l></block><block s="doReport"><block s="evaluate"><block s="reportJSFunction"><list></list><l>return this.finchDummy4;</l></block><list></list></block></block></script></block-definition><block-definition s="Finch Z Acceleration" type="reporter" category="sensing"><header></header><code></code><inputs></inputs><script><block s="doRun"><block s="reportJSFunction"><list></list><l>var sensorvalue = Number.MAX_VALUE;&#xD;&#xD;&#xD;finchAppID = "ojocioejjpmeccjfjlhkepchminjemod";&#xD;&#xD;&#xD;var myself = this;&#xD;&#xD;&#xD;&#xD;&#xD;&#xD;function getSensorData(callback) {&#xD;&#xD;&#xD;    var getValue = function(response) {&#xD;&#xD;&#xD;        sensorvalue = response.zAcc;&#xD;        callback(sensorvalue);&#xD;&#xD;&#xD;    };&#xD;&#xD;&#xD;&#xD;    console.log("sending message to chrome");&#xD;&#xD;&#xD;    chrome.runtime.sendMessage(finchAppID, {&#xD;&#xD;&#xD;        message: "POLL"&#xD;&#xD;&#xD;    }, getValue);&#xD;&#xD;&#xD;    console.log("sent message to chrome");&#xD;&#xD;&#xD;};&#xD;&#xD;&#xD;&#xD;&#xD;&#xD;function processValue(sensorvalue) {&#xD;&#xD;&#xD;    console.log("sensor value has been set: ", sensorvalue);&#xD;&#xD;&#xD;    myself.finchDummy5 